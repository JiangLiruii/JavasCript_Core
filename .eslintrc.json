{
    "extends": "airbnb-base",
    "rules": {
        "linebreak-style":["error","windows"],
        "no-alert":"off",
        "no-plusplus":0,
        "no-array-constructor":0,
        "no-unused-vars":0,
        "func-names":0,
        "no-console":0
    },
    "env": {
        "browser": true
    }
}